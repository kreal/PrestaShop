<fieldset style="width: 390px;">

<legend>

     {l s='WalletBit Information' mod='walletbit'}

</legend>

<div id="info">
	<table>
	<tr>
		<td align="left" valign="top">{l s='Batchnumber:' mod='walletbit'}</td>
		<td>{$batchnumber}</td>
	</tr>
	<tr>
		<td align="left" valign="top">{l s='Txid:' mod='walletbit'}</td>
		<td style="width: 317px; display: block; word-break: break-all; word-wrap: break-word;">{$txid}</td>
	</tr>
	<tr>
		<td align="left" valign="top">{l s='Address:' mod='walletbit'}</td>
		<td>{$address}</td>
	</tr>
	</table>
</div>

</fieldset>